	S17 -> S26  [color=black,style=bold,label="out_collect[numCollector]!1"];
	S19 -> S26  [color=black,style=bold,label="out_collect[numCollector]!0"];
	S23 -> S26  [color=black,style=bold,label="out_collect[numCollector]!2"];
