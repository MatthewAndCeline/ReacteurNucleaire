  GT -> S26;
	S26 -> S2  [color=black,style=bold,label="in_collect[numCollector]?_"];
	S2 -> S3  [color=black,style=solid,label="printf('Collecteur %d reçu signal ',numCollector)"];
	S3 -> S4  [color=black,style=bold,label="in_capteur[0]!0"];
	S4 -> S5  [color=black,style=bold,label="in_capteur[1]!0"];
	S5 -> S6  [color=black,style=bold,label="in_capteur[2]!0"];
	S6 -> S7  [color=black,style=bold,label="in_capteur[3]!0"];
	S7 -> S8  [color=black,style=bold,label="out_capteur[0]?valeur1"];
	S8 -> S9  [color=black,style=bold,label="out_capteur[1]?valeur2"];
