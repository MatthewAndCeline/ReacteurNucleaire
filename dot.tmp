digraph p_lanceur {
size="8,10";
  GT [shape=box,style=dotted,label="lanceur"];
  GT -> S1;
	S1 -> S2  [color=black,style=solid,label="(run Capteur(0))"];
	S2 -> S3  [color=black,style=solid,label="(run Capteur(1))"];
	S3 -> S4  [color=black,style=solid,label="(run Capteur(2))"];
	S4 -> S5  [color=black,style=solid,label="(run Capteur(3))"];
	S5 -> S6  [color=black,style=solid,label="(run Collector(0))"];
	S6 -> S7  [color=black,style=solid,label="(run Collector(1))"];
