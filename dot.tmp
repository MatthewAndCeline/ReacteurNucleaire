	S11 -> S12  [color=black,style=solid,label="printf('Collecteur %d reçu valeur %d du capteur 0 ',numCollector,valeur1)"];
	S12 -> S13  [color=black,style=solid,label="printf('Collecteur %d reçu valeur %d du capteur 1 ',numCollector,valeur2)"];
	S13 -> S14  [color=black,style=solid,label="printf('Collecteur %d reçu valeur %d du capteur 2 ',numCollector,valeur3)"];
	S14 -> S24  [color=black,style=solid,label="printf('Collecteur %d reçu valeur %d du capteur 3 ',numCollector,valeur4)"];
